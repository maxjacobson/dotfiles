[alias]
  br = "! git branch | cut -c 3- | selecta | xargs git checkout"
	co = checkout
	st = status
	hist = log --pretty=format:'%h %ai | %s%d [%an]' --graph --date=short
	l = log --pretty='[%h] [%ar] %an: %s'
[core]
	editor = vim
[color]
	ui = true
